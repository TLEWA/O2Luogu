// 随机数库函数
	random_float() // 生成一个 0 到 1 之间的随机浮点数
		return math(0,'random')
	random(a,b) // 生成一个 [a,b] 的随机整数
		return math(random_float()*(b - a + 1) + a, 'floor')
	// 生成一个随机的字母变量名
	random_name(size=8)
		name = ''
		for i in 1..size
			name += letter_map[random(0,25)]
		if (name in random_map) || is_defined(name)
			return random_name()
		random_map[name] = true
		return name
